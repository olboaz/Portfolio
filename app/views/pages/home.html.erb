  <div class="master-container home-vertical-height-adjust" id="services">
    <div class="section--default" data-ix="scroll-appear">
      <div id="about" data-scroll="0.6" class="section-intro">
        <div class="section-intro__heading">Services</div>
        <div class="separator-dashes">
          <div class="separator-dash-line"></div>
        </div>
      </div>

      <div class="section">
        <div class="service-cards">
          <div class="service-card">
            <i class="fas fa-code"></i><br><p>Web<br>Development</p>
          </div>
          <div class="service-card">
            <i class="fas fa-chart-bar"></i><br><p>Data<br>visualisation</p>
          </div>
          <div class="service-card">
            <i class="fas fa-database"></i><br><p>Data<br>analyse</p>
          </div>
          <div class="service-card">
            <i class="fas fa-graduation-cap"></i><br><p>Formation<br></p>
          </div>
        </div>
      </div>

    </div>
  </div>
<!-- ok -->

  <div class="master-container home-vertical-height-adjust" id="portfolio">
    <div class="section--default" data-ix="scroll-appear">
      <div id="about" data-scroll="0.6" class="section-intro">
        <div class="section-intro__heading">Portfolio</div>
        <div class="separator-dashes">
          <div class="separator-dash-line"></div>
        </div>
      </div>

      <div class="section">
        <div class="portfolio-cards">
          <% @projects.each do |project| %>
            <div class="portfolio-card">
              <%= link_to project.url, target: "_blank" do %>
                <div class="portfolio-img">
                  <%= cl_image_tag project.project_photo.key, width: 648, crop: :fit %>
                </div>
                <div class="portfolio-intro">
                  <h6><%= project.name %></h6>
                  <p><%= project.project_type %></p>
                </div>
              <% end %>
            </div>
          <% end %>
        </div>
      </div>

    </div>
  </div>
<!-- ok -->

  <div class="master-container home-vertical-height-adjust" id="skills">
    <div class="section--default" data-ix="scroll-appear">
      <div id="about" data-scroll="0.6" class="section-intro">
        <div class="section-intro__heading">Compétences</div>
        <div class="separator-dashes">
          <div class="separator-dash-line"></div>
        </div>
      </div>

      <div class="skill-cards">
        <% @tools.each do |tool| %>
          <div class="skill-card">
              <div class="skill-img" title="<%= tool.name.upcase %>">
                <%= cl_image_tag tool.tool_photo.key, width: 648, crop: :fit %>
              </div>
               <div class="overlay"><%= tool.name.upcase %></div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
<!-- ok -->

  <div class="master-container home-vertical-height-adjust" id="contact">
    <div class="section--default" data-ix="scroll-appear">
      <div id="about" data-scroll="0.6" class="section-intro">
        <div class="section-intro__heading">Contact</div>
        <div class="separator-dashes">
          <div class="separator-dash-line"></div>
        </div>
      </div>

      <div class="contact">
        <div>
          <h2>Me contacter</h2>
          <p>Une question ? Un projet ? Une formation ? ou un poste de rêve à me proposer ?<br> Contactez moi, je vous réponds !</p>
          <hr>
          <div class="social">
            <div class="social-icons">
              <a href="https://github.com/olboaz" target="_blank"><i class="fab fa-github"></i></a>
              <a href="https://www.linkedin.com/in/patrice-jaszczynski-66a18023/" target="_blank"><i class="fab fa-linkedin"></i></a>
              <a href="mailto:contact@pjaszczynski.fr?subject=Hello Patrice j'ai une demande !"><i class="far fa-envelope"></i></a>
              <a href="https://www.google.fr/maps/place/92300+Levallois-Perret/@48.8946561,2.2786462,15z/data=!3m1!4b1!4m5!3m4!1s0x47e66f83b73cffd3:0x40b82c3688b3db0!8m2!3d48.893217!4d2.287864" target="_blank"><i class="fas fa-map-marker-alt"></i></a>
            </div>
          </div>
        </div>

        <div>
          <div class="contact-form">
            <%= simple_form_for(@contact) do |f| %>
              <div class="row">
                <div class="col-6">
                  <div class="form-group">
                    <%= f.input :name, label: false, placeholder: "Nom et Prénom", class: "form-control"%>
                    <% if @name_error %>
                      <span class="message-error"><%= @name_error.first %></span>
                    <% end %>
                  </div>
                </div>
                <div class="col-6">
                  <div class="form-group">
                    <%= f.input :email,  label: false, placeholder: "Adresse email", class: "form-control" %>
                    <% if @email_error %>
                      <span class="message-error"><%= @email_error.first %></span>
                    <% end %>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <%= f.input :message,  label: false, as: :text, placeholder: "Votre message", class: "form-control" %>
                  <% if @message_error %>
                    <span class="message-error"><%= @message_error.first %></span>
                  <% end %>
              </div>
              <div class="actions">
                <%= f.submit "Contactez-moi"%>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
<!-- ok -->

</div>

